{

    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {

        "jsx-a11y/anchor-is-valid": "off",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "prettier/prettier": ["error", {
            "endOfLine": "auto"
        }]

    }
}